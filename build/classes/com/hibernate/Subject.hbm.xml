<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
		"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.beans.Subject" table="EXAM_ONLINE_SUBJECT_TABLE">
		
		<id name="Subjectid" type="java.lang.Integer">
			<column name="USERID" length="20" />
			<generator class="sequence">  
       			 <param name="sequence">SUBJECT_SEQUENCE</param>  
  		    </generator>
		</id>
		
		<property name="Subquestion" type="java.lang.String">
			<column name="USERNAME" length="1000"/>
		</property>
		<property name="Suboptions" type="java.lang.String">
			<column name="PASSWORD" length="1000"/>
		</property>
		
		<property name="Subanswer" type="java.lang.Integer">
			<column name="SCORE" length="20"/>
		</property>
		
	</class>

</hibernate-mapping>